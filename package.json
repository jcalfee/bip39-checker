{
  "name": "bip39-checker",
  "description": "The BIP-0039 wordlist spell check module.",
  "version": "1.0.4",
  "main": "lib/index.js",
  "license": "MIT",
  "scripts": {
    "test": "mocha src/index.test.js",
    "coverage": "istanbul cover _mocha -- -R spec src/index.test.js",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "build_lib": "node_modules/babel-cli/bin/babel.js src --out-dir lib",
    "prepublish": "npm run build_lib"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jcalfee/bip39-checker.git"
  },
  "dependencies": {
    "bip39": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "coveralls": "^2.13.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.1",
    "standard": "^10.0.2"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
